{
  "workspaceName": "alacbot",
  "version": "1.0.0",
  "description": "Multi-Agent AI System - Pi-Mono Compatible Configuration",
  "workspace": "~/alacbot",
  "_workspace_comment": "工作目录路径。默认为 ~/alacbot。修改此字段可切换工作目录，重启应用时会自动初始化新工作目录。支持 ~ 路径扩展。",
  "_comment": "This is a template config. Customize your API keys and workspace location",
  "storage": {
    "type": "markdown",
    "sessionDir": "./sessions",
    "configDir": "."
  },
  "providers": {
    "anthropic": {
      "baseUrl": "https://api.anthropic.com",
      "apiKeyEnv": "ANTHROPIC_API_KEY",
      "_apiKey_comment": "可选: 直接配置 API Key 明文 (不推荐用于生产环境, 仅用于开发测试)",
      "_apiKey_example": "sk-ant-xxx...",
      "api": "anthropic-messages",
      "models": [
        {
          "id": "claude-opus-4-1",
          "name": "Claude 3 Opus",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 15,
            "output": 75,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        },
        {
          "id": "claude-sonnet-4-20250514",
          "name": "Claude 3.5 Sonnet",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 3,
            "output": 15,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        },
        {
          "id": "claude-haiku-3-5",
          "name": "Claude 3.5 Haiku",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0.8,
            "output": 4,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        }
      ]
    },
    "openai": {
      "baseUrl": "https://api.openai.com/v1",
      "apiKeyEnv": "OPENAI_API_KEY",
      "api": "openai-responses",
      "models": [
        {
          "id": "gpt-4o",
          "name": "GPT-4o",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 2.5,
            "output": 10,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 128000,
          "maxTokens": 16384
        },
        {
          "id": "gpt-4-turbo",
          "name": "GPT-4 Turbo",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 10,
            "output": 30,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 128000,
          "maxTokens": 8192
        }
      ]
    },
    "modelscope": {
      "description": "ModelScope API - 支持中文大模型",
      "baseUrl": "https://api-inference.modelscope.cn/v1",
      "apiKeyEnv": "MODELSCOPE_API_KEY",
      "api": "openai-completions",
      "models": [
        {
          "id": "deepseek-ai/DeepSeek-V3",
          "name": "DeepSeek V3",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0,
            "output": 0,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 128000,
          "maxTokens": 32000
        }
      ]
    },
    "ollama": {
      "description": "本地 Ollama - 离线运行开源模型",
      "baseUrl": "http://localhost:11434/v1",
      "apiKeyEnv": "OLLAMA_API_KEY",
      "api": "openai-responses",
      "models": [
        {
          "id": "llama2",
          "name": "Llama 2",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0,
            "output": 0,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 4096,
          "maxTokens": 2048
        }
      ]
    },
    "custom": {
      "description": "自定义模型 - 任何 OpenAI 兼容的 API",
      "baseUrl": "http://your-api-server:port/v1",
      "apiKeyEnv": "CUSTOM_API_KEY",
      "api": "openai-responses",
      "models": [
        {
          "id": "your-model-id",
          "name": "Your Model Name",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0,
            "output": 0,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 4096,
          "maxTokens": 2048
        }
      ]
    }
  },
  "modelConfig": {
    "defaultProvider": "anthropic",
    "defaultModel": "claude-sonnet-4-20250514"
  },
  "agents": [
    {
      "name": "MainAgent",
      "enabled": true,
      "provider": "anthropic",
      "model": "claude-sonnet-4-20250514",
      "temperature": 0.7,
      "maxTokens": 2000,
      "skills": ["conversation", "reasoning", "context_management"],
      "description": "主对话代理 - 通用场景"
    },
    {
      "name": "CodeAgent",
      "enabled": true,
      "provider": "anthropic",
      "model": "claude-opus-4-1",
      "temperature": 0.2,
      "maxTokens": 4000,
      "skills": ["code_analysis", "debugging", "optimization"],
      "description": "代码分析和调试 - 需要更强的推理能力"
    },
    {
      "name": "CreativeAgent",
      "enabled": false,
      "provider": "anthropic",
      "model": "claude-sonnet-4-20250514",
      "temperature": 0.9,
      "maxTokens": 3000,
      "skills": ["content_generation", "brainstorming", "creative_writing"],
      "description": "创意写作 - 高温度生成多样化内容"
    }
  ],
  "features": {
    "multiTurn": true,
    "sessionPersistence": true,
    "markdownStorage": true,
    "agentRouting": true,
    "modelSwitching": true,
    "costTracking": true
  },
  "defaults": {
    "sessionTimeout": 3600,
    "maxHistoryLength": 100,
    "autoSave": true,
    "autosaveInterval": 30000
  }
}
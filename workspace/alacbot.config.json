{
  "workspaceName": "alacbot",
  "version": "1.0.0",
  "description": "Multi-Agent AI System with Model Configuration",
  "storage": {
    "type": "markdown",
    "sessionDir": "./sessions",
    "configDir": "."
  },
  "providers": {
    "anthropic": {
      "baseUrl": "https://api.anthropic.com",
      "apiKeyEnv": "ANTHROPIC_API_KEY",
      "api": "anthropic-messages",
      "models": [
        {
          "id": "claude-opus-4-1",
          "name": "Claude 3 Opus",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 15,
            "output": 75,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        },
        {
          "id": "claude-sonnet-4-20250514",
          "name": "Claude 3.5 Sonnet",
          "reasoning": false,
          "input": ["text", "image"],
          "cost": {
            "input": 3,
            "output": 15,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        },
        {
          "id": "claude-haiku-3-5",
          "name": "Claude 3.5 Haiku",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0.8,
            "output": 4,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 200000,
          "maxTokens": 8192
        }
      ]
    },
    "custom": {
      "description": "自定义模型 (本地 LLM、私有 API 等)",
      "baseUrl": "http://localhost:8000/v1",
      "apiKeyEnv": "CUSTOM_MODEL_API_KEY",
      "api": "openai-responses",
      "models": [
        {
          "id": "my-custom-model",
          "name": "My Custom Model",
          "reasoning": false,
          "input": ["text"],
          "cost": {
            "input": 0,
            "output": 0,
            "cacheRead": 0,
            "cacheWrite": 0
          },
          "contextWindow": 4096,
          "maxTokens": 2048
        }
      ]
    }
  },
  "modelConfig": {
    "defaultProvider": "anthropic",
    "defaultModel": "claude-sonnet-4-20250514"
  },
  "agents": [
    {
      "name": "MainAgent",
      "enabled": true,
      "provider": "anthropic",
      "model": "claude-sonnet-4-20250514",
      "temperature": 0.7,
      "maxTokens": 2000,
      "skills": ["conversation", "reasoning", "context_management"],
      "description": "Main conversational agent"
    }
  ],
  "features": {
    "multiTurn": true,
    "sessionPersistence": true,
    "markdownStorage": true,
    "agentRouting": true,
    "modelSwitching": true,
    "costTracking": true
  },
  "defaults": {
    "sessionTimeout": 3600,
    "maxHistoryLength": 100,
    "autoSave": true,
    "autosaveInterval": 30000
  }
}
{
  "workspaceName": "mini-openclaw",
  "version": "1.0.0",
  "description": "Multi-Agent AI System with Model Configuration",
  "storage": {
    "type": "markdown",
    "sessionDir": "./sessions",
    "configDir": "."
  },
  "modelConfig": {
    "default": "claude-sonnet-4-20250514",
    "defaultProvider": "anthropic",
    "providers": [
      {
        "name": "anthropic",
        "apiKeyEnv": "ANTHROPIC_API_KEY",
        "baseUrl": "https://api.anthropic.com",
        "models": [
          {
            "id": "claude-opus-4-1",
            "name": "Claude 3 Opus",
            "description": "Most capable model for complex tasks",
            "contextWindow": 200000,
            "costPerMTok": {
              "input": 15,
              "output": 75
            },
            "capabilities": ["vision", "tool_use", "thinking"]
          },
          {
            "id": "claude-sonnet-4-20250514",
            "name": "Claude 3.5 Sonnet",
            "description": "Balanced model for general tasks",
            "contextWindow": 200000,
            "costPerMTok": {
              "input": 3,
              "output": 15
            },
            "capabilities": ["vision", "tool_use"]
          },
          {
            "id": "claude-haiku-3-5",
            "name": "Claude 3.5 Haiku",
            "description": "Fastest and most compact model",
            "contextWindow": 200000,
            "costPerMTok": {
              "input": 0.8,
              "output": 4
            },
            "capabilities": ["tool_use"]
          }
        ]
      },
      {
        "name": "openai",
        "apiKeyEnv": "OPENAI_API_KEY",
        "baseUrl": "https://api.openai.com/v1",
        "models": [
          {
            "id": "gpt-4o",
            "name": "GPT-4 Optimized",
            "description": "Latest GPT-4 Optimized model",
            "contextWindow": 128000,
            "costPerMTok": {
              "input": 5,
              "output": 15
            },
            "capabilities": ["vision", "tool_use"]
          },
          {
            "id": "gpt-4-turbo",
            "name": "GPT-4 Turbo",
            "description": "GPT-4 with better performance",
            "contextWindow": 128000,
            "costPerMTok": {
              "input": 10,
              "output": 30
            },
            "capabilities": ["vision", "tool_use"]
          },
          {
            "id": "gpt-3.5-turbo",
            "name": "GPT-3.5 Turbo",
            "description": "Fast and cost-effective model",
            "contextWindow": 16000,
            "costPerMTok": {
              "input": 0.5,
              "output": 1.5
            },
            "capabilities": ["tool_use"]
          }
        ]
      },
      {
        "name": "google",
        "apiKeyEnv": "GOOGLE_API_KEY",
        "baseUrl": "https://generativelanguage.googleapis.com",
        "models": [
          {
            "id": "gemini-2.0-flash",
            "name": "Gemini 2.0 Flash",
            "description": "Latest Gemini Flash model",
            "contextWindow": 1000000,
            "costPerMTok": {
              "input": 0.075,
              "output": 0.3
            },
            "capabilities": ["vision", "tool_use"]
          },
          {
            "id": "gemini-1.5-pro",
            "name": "Gemini 1.5 Pro",
            "description": "Gemini Pro with long context",
            "contextWindow": 1000000,
            "costPerMTok": {
              "input": 1.25,
              "output": 5
            },
            "capabilities": ["vision", "tool_use"]
          }
        ]
      },
      {
        "name": "ollama",
        "apiKeyEnv": null,
        "baseUrl": "http://localhost:11434",
        "isLocal": true,
        "models": [
          {
            "id": "llama2",
            "name": "Llama 2",
            "description": "Local Llama 2 model",
            "contextWindow": 4096,
            "costPerMTok": {
              "input": 0,
              "output": 0
            },
            "capabilities": ["tool_use"]
          },
          {
            "id": "mistral",
            "name": "Mistral",
            "description": "Local Mistral model",
            "contextWindow": 8192,
            "costPerMTok": {
              "input": 0,
              "output": 0
            },
            "capabilities": ["tool_use"]
          }
        ]
      }
    ]
  },
  "agents": [
    {
      "name": "MainAgent",
      "enabled": true,
      "model": "anthropic",
      "modelId": "claude-sonnet-4-20250514",
      "temperature": 0.7,
      "maxTokens": 2000,
      "skills": ["conversation", "reasoning", "context_management"],
      "description": "Main conversational agent"
    },
    {
      "name": "CodeAgent",
      "enabled": true,
      "model": "anthropic",
      "modelId": "claude-opus-4-1",
      "temperature": 0.3,
      "maxTokens": 4000,
      "skills": ["code_generation", "code_review", "debugging"],
      "description": "Code specialist",
      "triggers": ["code", "programming", "debug"]
    },
    {
      "name": "AnalysisAgent",
      "enabled": true,
      "model": "openai",
      "modelId": "gpt-4o",
      "temperature": 0.5,
      "maxTokens": 3000,
      "skills": ["data_analysis", "visualization", "reporting"],
      "description": "Data analysis expert",
      "triggers": ["analyze", "data", "trends"]
    }
  ],
  "features": {
    "multiTurn": true,
    "sessionPersistence": true,
    "markdownStorage": true,
    "agentRouting": true,
    "modelSwitching": true,
    "costTracking": true
  },
  "defaults": {
    "sessionTimeout": 3600,
    "maxHistoryLength": 100,
    "autoSave": true,
    "autosaveInterval": 30000
  }
}